# ls color
case $OSENV in
linux|cygwin)
  if which dircolors >/dev/null; then
    test -r ~/.dircolors && eval "$(dircolors -b $HOME/.dircolors)" || eval "$(dircolors -b)"
  fi
  ;;
macosx)
  export LSCOLORS="gxdxeaeacxdadahbadxcec"
  ;;
esac

