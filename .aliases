alias la='ls -aF --color=tty'
alias lt='ls -At --color=tty'
alias ll='ls -alFh --color=tty'
alias rm='rm -i'
alias mv='mv -i'
alias cp='cp -i'
alias df='df -h'
alias du='du -h'
alias du1='du -h --max-depth=1'
alias diff='diff -u'
alias diffy='diff -y'
alias cdiff='colordiff -u'
alias cdiffy='colordiff -y'
alias less='less -iSR'
alias tab='column -t -s"	"'
alias whence='type -a'
alias grep='grep --color --binary-file=without-match'
alias rdiff='rsync -nacOzivHS --delete'
alias new='ll -t | head'

alias R='R --quiet'

